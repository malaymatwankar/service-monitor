<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>
<main>
<form method="POST" action="/service/remove">
<%  services.forEach(function(service){ %>
  <div class="checkbox">
       <label>
            <input type="checkbox" id="<%= service.name+service.env+'Checkbox' %>" name="<%= service.name+service.env+'Checkbox' %>" />
            <%= service.name+'-'+service.env %>
       </label>
   </div>
<% }) %>
<input type="submit" value="Delete" />
</form>



</main>
<footer>
    <% include ../partials/footer %>
</footer>
